{
  "name": "react-boilerplate-ts",
  "version": "1.0.0",
  "description": "A react/redux boilerplate with typescript to create new application",
  "author": "Frederic Godin",
  "repository": "Epimodev/react-boilerplate-ts",
  "license": "MIT",
  "main": "src/index.tsx",
  "keywords": [
    "react",
    "redux",
    "webpack",
    "svg-sprite",
    "css modules",
    "typescript",
    "tslint"
  ],
  "scripts": {
    "dev": "node webpack/development.js",
    "build": "yarn lint && node webpack/build.js",
    "lint": "tslint -c tslint.json 'src/**/*.ts?(x)'",
    "check-format": "prettier --list-different 'src/**/*.ts?(x)'",
    "storybook": "start-storybook -s ./ -p 9001 -c .storybook"
  },
  "devDependencies": {
    "@storybook/react": "^3.4.7",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.2",
    "autoprefixer": "^8.6.2",
    "awesome-typescript-loader": "^5.0.0",
    "babel-core": "^6.26.3",
    "css-loader": "^0.28.11",
    "dotenv": "^6.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.2",
    "node-sass": "^4.13.1",
    "postcss-loader": "^2.1.5",
    "precise-commits": "^1.0.2",
    "prettier": "^1.13.5",
    "sass-loader": "^7.0.3",
    "shelljs": "^0.8.1",
    "style-loader": "^0.21.0",
    "svg-sprite-html-webpack": "^2.3.0",
    "ts-loader": "^3.5.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.8.0",
    "typescript": "^2.9.2",
    "webpack": "^4.12.0",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3",
    "webpackbar": "^2.6.1"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn check-format"
    }
  }
}
